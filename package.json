{
  "name": "vaccination-service",
  "version": "1.0.0",
  "description": "API REST para coordinar registros de vacunaci√≥n",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "fix-tslint": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RanKey1496/vaccination-service.git"
  },
  "keywords": [],
  "author": "rankey1496",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RanKey1496/vaccination-service/issues"
  },
  "homepage": "https://github.com/RanKey1496/vaccination-service#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "http-status": "^1.5.3",
    "inversify": "^6.0.1",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.11",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/errorhandler": "^1.5.0",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "concurrently": "^7.6.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  }
}
